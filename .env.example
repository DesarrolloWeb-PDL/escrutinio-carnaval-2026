# Configuración de base de datos para Prisma
# Necesitas una base de datos PostgreSQL. Puedes usar Vercel Postgres, Supabase, Neon, etc.
DATABASE_URL="postgresql://usuario:password@host:port/database?schema=public"

# Configuración del servidor (opcional, para desarrollo local)
PORT=3000

# URL de la API para el frontend (si el backend está en otro dominio, sino dejar vacío o usar /api)
VITE_API_URL="/api"
